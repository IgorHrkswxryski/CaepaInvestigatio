node.highlighted {
	min-zoomed-font-size: 0;
    font-size: 10;
    text-outline-width: 2;
    text-outline-opacity: 1;
    z-index: 9999;
}
 
edge.highlighted {
	opacity: 0.8;
	width: 3;
	z-index: 9999;
}
 
edge.faded {
	opacity: 0.1;
}
 
node.faded {
	opacity: 0.5;
	text-opacity: 0;
}
 
node:selected {
